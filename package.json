{
  "name": "wollok-js",
  "version": "0.2.0",
  "description": "JS based Wollok engine",
  "license": "GPL-3.0",
  "repository": "uqbar-project/wollok-js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --colors --require babel-register --reporter min test/**.test.js",
    "lint": "eslint src/**.js test/**.js",
    "coverage-test": "NODE_ENV=test nyc --require babel-core/register mocha --compilers js:babel-register --recursive tests",
    "coverage-report": "nyc report --reporter=lcov",
    "coverage-upload": "codecov"
  },
  "dependencies": {
    "color-log": "0.0.2",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-also": "^0.1.0",
    "codecov": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.1",
    "nyc": "^11.0.3"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-function-bind"
    ]
  }
}
